{
  "name": "topi_server",
  "version": "1.0.0",
  "main": "dist/topi_main",
  "types": "dist/topi_main",
  "scripts": {
    "build": "./build.sh",
    "build:watch": "npx onchange -v 'src/**/*.ts' -- ./watch.sh",
    "lint": "npx eslint . --ext .ts",
    "test": "npx jest --forceExit --verbose",
    "test:watch": "npx jest --watch",
    "//": "Create a relocateable tgz with api code",
    "bundle": "./bundle.sh"
  },
  "license": "UNLICENSED",
  "files": [
    "schema",
    "build.sh",
    "dist",
    ".yalc",
    "*.yaml",
    "yarn.lock"
  ],
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "ts_node smokes and smolders",
          "outputPath": "test/reports/default-report.html"
        }
      ]
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec|smoke|smolder))\\.(ts|tsx|js)$",
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "gql"
    ],
    "modulePaths": [
      "node_modules",
      "../../../app/ts_node/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false
  },
  "keywords": [],
  "author": "skelly@sagaeducation.org",
  "dependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/minimist": "^1.2.1",
    "@types/request": "^2.48.5",
    "ajv": "^7.2.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "1.4.5",
    "date-fns": "^2.17.0",
    "delay": "^5.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "guid-typescript": "^1.0.9",
    "lodash": "^4.17.20",
    "minimist": "^1.2.6",
    "mongodb": "^3.6.3",
    "mysql": "^2.18.1",
    "require-text": "^0.0.1",
    "sha1": "^1.1.1",
    "treant-js": "^1.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yaml": "^1.10.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.7",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/sha1": "^1.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.32.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "nodemon": "^2.0.7",
    "onchange": "^7.1.0",
    "random-email": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-json-schema-generator": "^0.88.0",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  }
}
